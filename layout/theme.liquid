    <script>
      var layountShiftEntries = []

      window.addEventListener("load", (event) => {
        if (layountShiftEntries.length) {
          const debugTarget = getCLSDebugTarget(layountShiftEntries);

          if (!debugTarget) return;

            console.log({
              'LayoutShift source': debugTarget.largestSource.node,
              'Largest Entry Score': debugTarget.largestEntryScore,
              'Remaining Sources Count': debugTarget.remainingSourcesCount,
            })

            console.log('currentRect: ', debugTarget.largestSource.currentRect)
            console.log('previousRect: ', debugTarget.largestSource.previousRect)
        }
      });

      const observer = new PerformanceObserver((list) => {
        for (const entry of list.getEntries()) {
          // Count layout shifts without recent user input only
          if (!entry.hadRecentInput) {
            layountShiftEntries.push(entry)
          }
        }
      });

      observer.observe({ type: "layout-shift", buffered: true });

      function getCLSDebugTarget(entries) {
        const largestEntry = entries.reduce((a, b) =>
          a && a.value > b.value ? a : b,
        );
        if (largestEntry?.sources?.length) {
          const largestSource = largestEntry.sources.reduce((a, b) => {
            const area = (el) => el.previousRect.width * el.previousRect.height;
            return a.node && area(a) > area(b) ? a : b;
          });
          if (largestSource) {
            return {
                largestSource: largestSource,
                largestEntryScore: largestEntry.value,
                remainingSourcesCount: largestEntry.sources.length
              };
          }
        }
      }
    </script>

